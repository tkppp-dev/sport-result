{
  "name": "node-backend",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "export NODE_ENV=production && pm2 start ts-node -- -r tsconfig-paths/register ./bin/www.ts",
    "dev": "export NODE_OPTIONS='--trace-deprecation --abort-on-uncaught-exception' && export NODE_ENV=development && nodemon --exec ts-node ./bin/www.ts",
    "build": "sh etc/build.sh"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "cheerio": "^1.0.0-rc.12",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^16.0.0",
    "express": "~4.16.1",
    "express-async-errors": "^3.1.1",
    "log4js": "latest",
    "module-alias": "^2.2.2",
    "moment": "^2.29.3",
    "moment-timezone": "^0.5.34",
    "morgan": "~1.9.1",
    "mysql2": "^2.3.3",
    "node-schedule": "^2.1.0",
    "puppeteer": "^15.3.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "typeorm": "^0.3.7"
  },
  "devDependencies": {
    "@tsconfig/recommended": "^1.0.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/debug": "^4.1.7",
    "@types/express": "^4.17.13",
    "@types/http-errors": "^1.8.1",
    "@types/morgan": "^1.9.3",
    "@types/node": "^16.10.2",
    "@types/node-schedule": "^2.1.0",
    "nodemon": "^2.0.15",
    "ts-node": "^10.2.1",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.4.3"
  },
  "_moduleAliases": {
    "@": "./src"
  }
}
